<Window x:Class="AvaloniaManager.MainWindow"
        xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:notifications="using:Avalonia.Controls.Notifications"
		xmlns:vm="clr-namespace:AvaloniaManager.ViewModels"
        xmlns:local="clr-namespace:AvaloniaManager"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Width="1920" Height="1080" MinWidth="720"
        Title="Avalonia Manager"
        Icon="/Assets/кчау.png"
        x:DataType="vm:MainWindowViewModel">
	<local:MainView DataContext="{Binding}"/>
</Window>